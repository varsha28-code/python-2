class Solution:
    def dfs(self, adj):
        def dfs_util(node, vis, adj, ans):
            ans.append(node)
            for i in adj[node]:
                if vis[i] == 0:
                    vis[i] = 1
                    dfs_util(i, vis, adj, ans)

        n = len(adj)
        vis = [0] * n
        ans = []

        startNode = 0
        vis[startNode] = 1
        dfs_util(startNode, vis, adj, ans)

        return ans
