def createBST(arr):
    root=None
    for data in arr:
        if(root==None):
            root=Node(data)
        else:
            curr=root
            while(True):
                if(data<curr.data):
                    if(curr.left==None):
                        curr.left=Node(data)
                        break
                    else:
                        curr=curr.left
                elif(data>curr.data):
                    if(curr.right==None):
                        curr.right=Node(data)
                        break
                    else:
                        curr=curr.right
    return root.left.data 
